<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    tools:context=".RTSP1Activity"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:keepScreenOn="true">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/linear1"
        android:orientation="horizontal"
        android:layout_weight="1">
        <org.videolan.libvlc.util.VLCVideoLayout
            android:id="@+id/videoLayout1"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1" />

        <org.videolan.libvlc.util.VLCVideoLayout
            android:id="@+id/videoLayout2"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1" />
        <org.videolan.libvlc.util.VLCVideoLayout
            android:id="@+id/videoLayout3"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/linear2"
        android:orientation="horizontal"
        android:layout_weight="1">
        <org.videolan.libvlc.util.VLCVideoLayout
            android:id="@+id/videoLayout4"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1" />

        <org.videolan.libvlc.util.VLCVideoLayout
            android:id="@+id/videoLayout5"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1" />
        <org.videolan.libvlc.util.VLCVideoLayout
            android:id="@+id/videoLayout6"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/linear3"
        android:orientation="horizontal"
        android:layout_weight="1">
        <org.videolan.libvlc.util.VLCVideoLayout
            android:id="@+id/videoLayout7"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1" />

        <org.videolan.libvlc.util.VLCVideoLayout
            android:id="@+id/videoLayout8"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1" />
        <org.videolan.libvlc.util.VLCVideoLayout
            android:id="@+id/videoLayout9"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1" />
    </LinearLayout>

</LinearLayout>